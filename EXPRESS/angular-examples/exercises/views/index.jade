doctype html
html
	head
	link(rel="stylesheet",href="/bower_components/flatstrap/dist/css/flatstrap.min.css")
	body
		header
		section
			#web
				.container
					.row
						.col-md-4.col-md-offset-2
							.panel.panel-primary
								.panel-heading
									|Hi please click on the file
								.panel-body
									ul
									each link in ['ng-class','ngAnimate3','ngAnimate2','ngAnimate1','provider-example','watch-example','form','angular-ui1','angular-ui2','q','q_2','filter','filter_1','shadowDOM','materials','polymer','polymer1','customElements','customElements+shadowDOM','customEle_shadowDOM_2']
										li.list-inline
											a.btn.btn-lg.btn-block.btn-default(href="#{link}")
												i.glyphicon.glyphicon-file
												span.col-sm-offset-1|#{link}.jade
